<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AntrianPage">

    <TextView
        android:id="@+id/judul"
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="ANTRIAN"
        android:textSize="40dp"/>
    <TextView
        android:layout_below="@id/judul"
        android:id="@+id/nomorantriansaatini"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="071"
        android:textSize="130dp"
        android:layout_centerHorizontal="true"/>
    <TextView
        android:layout_below="@id/nomorantriansaatini"
        android:id="@+id/noantrian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No anda : 80"
        android:layout_centerHorizontal="true"
        android:textSize="40dp"
        />
    <TextView
        android:layout_below="@id/noantrian"
        android:layout_marginTop="50dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="30dp"
        android:text="Estimasi\n50 menit"/>

</RelativeLayout>